		"@tailwindcss/typography": "^0.5.2",
		"@types/http-server": "^0.12.1",
		"@types/lodash": "^4.14.182",
		"@types/marked": "^4.0.3",
		"@types/pluralize": "^0.0.29",
		"@types/to-ico": "^1.1.1",
